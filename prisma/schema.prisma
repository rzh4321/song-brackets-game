datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  userId        Int       @id @default(autoincrement())
  username  String    @unique
  password  String?
  name      String
  spotifyUserId    String?   @unique
  playlists Playlist[]
}

model Playlist {
  playlistId Int    @id @default(autoincrement())
  userId     Int
  playCount  Int
  highScore  Int
  user       User   @relation(fields: [userId], references: [userId])
}